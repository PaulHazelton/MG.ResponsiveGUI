	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/Veedja.MG.ResponsiveGui.Sample/bin/Debug/net6.0/Veedja.MG.ResponsiveGui.Sample.exe",
			"args": [],
			"cwd": "${workspaceFolder}/Veedja.MG.ResponsiveGui.Sample",
			"console": "integratedTerminal",
			"stopAtEntry": false,
			"internalConsoleOptions": "neverOpen",
			"justMyCode": false,
			"suppressJITOptimizations": true,
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach"
		}
	]
}